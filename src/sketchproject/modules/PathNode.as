package sketchproject.modules {		/**	 * Save node of map when used in PathFinder.	 * 	 * @author Angga	 */	public class PathNode 	{		public var x:Number;		public var y:Number;		public var g:Number;		public var h:Number;		public var parentNode:PathNode;				/**		 * Default constructor of PathNode.		 * 		 * @param xPos current x position (row)		 * @param yPos current y position (column)		 * @param gVal real distance from root parent to current node		 * @param hVal heuristic value		 * @param link parent node		 */		public function PathNode(xPos:Number, yPos:Number, gVal:Number, hVal:Number, link:PathNode):void {			x = xPos;			y = yPos;			g = gVal;			h = hVal;			parentNode = link;		}			}}